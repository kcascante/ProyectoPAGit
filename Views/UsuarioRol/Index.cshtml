@model IEnumerable<ProyectoPA.Models.UsuarioRol>

@{
    ViewBag.Title = "Index";
}

    <h2>UsuarioRol</h2>

<p>
    @Html.ActionLink("Crear UsuarioRol", "Crear")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdUsuarioRol)
        </th>
        <th>
